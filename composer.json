{
    "name": "janwebdev/finko-queue-bundle",
    "type": "ideasbucket-queue-bundle",
    "description": "IdeasBucket QueueBundle fork for Symfony Framework 4+",
    "keywords": ["finko","queue","symfony 4"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/nicholasnet/QueueBundle/issues",
        "source": "https://github.com/nicholasnet/QueueBundle"
    },
    "require": {
        "php": ">=5.6.0",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "symfony/framework-bundle": "^2.7 || ^3.0 || ^4.0",
        "symfony/console": "~2.5|~3.0|~4.0",
        "ideasbucket/common": "~1.1",
        "psr/simple-cache": "1.0.0",
        "symfony/process": "^2.7 || ^3.0 || ^4.0"
    },
    "require-dev": {
        "aws/aws-sdk-php": "^3.0",
        "pda/pheanstalk": "^3.0",
        "phpunit/phpunit": "^8.0",
        "predis/predis": "~1.1.1",
        "mockery/mockery": "~1.2.0"
    },
    "autoload": {
        "psr-4": { "Finko\\QueueBundle\\": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "prefer-stable": true
}
